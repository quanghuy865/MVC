@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    var Listuser = (List<Model.DbConection.User>)ViewBag.Users;
}

<div class="row">
    <div class="col-lg-12">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>UserName</th>
                    <th>UserCode</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Listuser)
                {
                    <tr>
                        <td>@item.UserCode</td>
                        <td>@item.FullName</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>